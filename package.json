{
  "name": "@nish1896/eslint-config-root",
  "version": "2.1.4",
  "description": "eslint and stylistic rules to help you focus more on the code logic, while they take care of the code formatting",
  "author": "Nishant Kohli",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*",
    "apps/*"
  ],
  "scripts": {
    "config:link": "yarn workspace @nish1896/eslint-config run link:local",
    "flat-config:link": "yarn workspace @nish1896/eslint-flat-config run link:local",
    "config:unlink": "npm unlink @nish1896/eslint-config",
    "flat-config:unlink": "npm unlink @nish1896/eslint-flat-config",
    "esdocs": "yarn workspace @nish1896/eslint-config-docs run start",
    "docs:build": "yarn workspace @nish1896/eslint-config-docs run build",
    "js-8:lint": "yarn workspace js-eslint-v8 run lint",
    "js-9:lint": "yarn workspace js-eslint-v9 run lint",
    "next-v8:lint": "yarn workspace next-eslint-v8 run lint",
    "react-v9:lint": "yarn workspace react-eslint-v9 run lint",
    "vite-v9:lint": "yarn workspace vite-eslint-v9 run lint"
  },
  "devDependencies": {
    "rimraf": "^6.0.1"
  },
  "keywords": [
    "@nish1896",
    "@nish1896/eslint-config",
    "eslint",
    "eslintconfig",
    "eslint-config",
    "eslint-config-jsx",
    "eslint-config-react",
    "eslint-react",
    "lint-config",
    "lint-rules",
    "react-eslint"
  ],
  "homepage": "https://nish1896-eslint-config.vercel.app/",
  "repository": {
    "url": "git+https://github.com/nishkohli96/eslint-config.git"
  },
  "license": "MIT"
}
